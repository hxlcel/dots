    { device = "/dev/disk/by-uuid/9F74-DAF2";
      fsType = "vfat";
      options = [ "fmask=0077" "dmask=0077" ];
    };

  swapDevices =
    [ { device = "/dev/disk/by-uuid/c0579802-1902-4fb9-a780-1b6279a83a46"; }
